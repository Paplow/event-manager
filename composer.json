{
    "name": "paplow/event-manager",
    "description": "A simple event manager module for your laravel app.",
    "type": ["library", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lawrence Onah Jnr",
            "email": "paplow01@gmail.com"
        }
    ],
    "repository": "https://github.com/paplow/event-manager",
    "require": {
        "php": ">=7.0",
        "illuminate/*": "~5.0",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-0": {
            "Paplow\\EventManager": "src/EventManager"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paplow\\EventManager\\EventServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
